{
  "version": 2,
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/react"
    },
    {
      "src": "backend/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/client/build/$1"
    },
    {
      "src": "/bfhl",
      "dest": "/backend/server.js"
    }
  ]
}
